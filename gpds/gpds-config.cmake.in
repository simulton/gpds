include(CMakeFindDependencyMacro)

set(GPDS_DEPENDENCY_TINYXML2_FINDPACKAGE    @GPDS_DEPENDENCY_TINYXML2_FINDPACKAGE@)
set(GPDS_DEPENDENCY_TINYXML2_PKGCONFIG      @GPDS_DEPENDENCY_TINYXML2_PKGCONFIG@)

if(GPDS_DEPENDENCY_TINYXML2_FINDPACKAGE)
    find_dependency(tinyxml2 REQUIRED)
endif()
if(GPDS_DEPENDENCY_TINYXML2_PKGCONFIG)
    find_dependency(PkgConfig REQUIRED)
    pkg_check_modules(tinyxml2 REQUIRED IMPORTED_TARGET tinyxml2)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/gpds-targets.cmake")
